{
    "name": "spinen/ncentral-php-client",
    "description": "SPINEN's PHP Client for N-able's N-Central.",
    "keywords": [
        "N-able",
        "N-Central",
        "client",
        "spinen"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jimmy Puckett",
            "email": "jimmy.puckett@spinen.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": "~6.3|~7.4",
        "illuminate/support": "~7.0|~8.0",
        "nesbot/carbon": "~2.13",
        "phpro/soap-client": "^1.7"
    },
    "require-dev": {
        "laminas/laminas-code": "^3.5",
        "mockery/mockery": "~1.3.2",
        "phpunit/phpunit": "~9.0",
        "psy/psysh": "~0.10"
    },
    "autoload": {
        "psr-4": {
            "Spinen\\Nable\\Ncentral\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spinen\\Nable\\Ncentral\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
